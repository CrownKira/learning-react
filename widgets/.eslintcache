    return () => {\n            // this will get invoked before the Component is removed from the dom \n            document.body.removeEventListener('click', onBodyClick);\n        };\n    }, []);\n\n    const renderedOptions = options.map((option) => {\n        if (option.value === selected.value) {\n            return null;\n        }\n        return (\n            <div\n                key={option.value}\n                className=\"item\"\n                onClick={() => onSelectedChange(option)}\n            >\n                {option.label}\n            </div>\n        );\n    })\n    return (\n        <div ref={ref} className=\"ui form \">\n            <div className=\"field\">\n                <label htmlFor=\"\" className=\"label\">{label}</label>\n                {/* open or close on click */}\n                <div onClick={() => setOpen(!open)} className={`ui selection dropdown ${open ? 'visible active' : ''}`}>\n                    <i className=\"dropdown icon\"></i>\n                    <div className=\"text\">{selected.label}</div>\n                    <div className={`menu ${open ? 'visible transition' : ''}`}>\n                        {/* change the selected on click */}\n                        {renderedOptions}\n                    </div>\n                </div>\n            </div>\n        </div >\n    );\n};\n\nexport default Dropdown;","/Users/kylekira/Desktop/React Project/React projects/widgets/src/components/Translate.js",[],"/Users/kylekira/Desktop/React Project/React projects/widgets/src/components/Convert.js",[],"/Users/kylekira/Desktop/React Project/React projects/widgets/src/components/Route.js",[],"/Users/kylekira/Desktop/React Project/React projects/widgets/src/components/Header.js",[],"/Users/kylekira/Desktop/React Project/React projects/widgets/src/components/Link.js",[],{"ruleId":"50","severity":1,"message":"51","line":1,"column":17,"nodeType":"52","messageId":"53","endLine":1,"endColumn":26},{"ruleId":"50","severity":1,"message":"5